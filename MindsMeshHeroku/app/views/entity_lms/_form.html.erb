<%= simple_form_for(@entity_lm) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :lms_provider_id %>
    <%= f.input :entity_id %>
    <%= f.input :version %>
    <%= f.input :host %>
    <%= f.input :secure %>
    <%= f.input :lti-guid %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
