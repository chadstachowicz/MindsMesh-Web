<h1>Listing lms_providers</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lms_providers.each do |lms_provider| %>
  <tr>
    <td><%= lms_provider.name %></td>
    <td><%= link_to 'Show', lms_provider %></td>
    <td><%= link_to 'Edit', edit_lms_provider_path(lms_provider) %></td>
    <td><%= link_to 'Destroy', lms_provider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

