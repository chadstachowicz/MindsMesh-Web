<% @feedback_bugs.each do |feedback| %>
  <tr>
    <td><%= feedback.request_type %></td>
    <td><%= feedback.platform %></td>
    <td><%= feedback.user_id %></td>
    <td><%= link_to 'Show', feedback %></td>
    <td><%= link_to 'Edit', edit_feedback_bug_path(feedback) %></td>
    <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

