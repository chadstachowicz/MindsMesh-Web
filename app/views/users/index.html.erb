<h1>Listing users</h1>

<table>
  <tr>
    <th></th>
    <th>Name</th>
    <th>Creation</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= image_tag user.photo_url, width: 25 %></td>
    <td><%= link_to user.name, user %></td>
    <td><%= l user.created_at %></td>
  </tr>
<% end %>
</table>