<h1>Listing section_users</h1>

<table>
  <tr>
    <th>Section</th>
    <th>User</th>
    <th>Role</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @section_users.each do |section_user| %>
  <tr>
    <td><%= section_user.section %></td>
    <td><%= section_user.user %></td>
    <td><%= section_user.role %></td>
    <td><%= link_to 'Show', section_user %></td>
    <td><%= link_to 'Edit', edit_section_user_path(section_user) %></td>
    <td><%= link_to 'Destroy', section_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

