<%= simple_form_for(@admin_newsletter) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :htmlemail, cols: 80, rows: 12 %>
    <%= f.input :status, :label => 'Published' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
